<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- ROS 2 plugin to allow robot spawning -->
    <plugin name="gazebo_ros_factory" filename="libgazebo_ros_factory.so"/>
    
  </world>
</sdf>
